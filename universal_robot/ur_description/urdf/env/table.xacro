<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">

<material name="yellow">
  <color rgba="1 1 0 1"/>
</material>


<xacro:macro name="table" params="name">
  <link name="table">
    <visual>
      <geometry>
        <box size="0.5 0.5 0.05"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <box size="0.5 0.5 0.05"/>
      </geometry>
    </collision>
  </link>
</xacro:macro>

</robot>

